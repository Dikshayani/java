package operators;

public class arithmatic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int a = 10;
      int b = 15;
      System.out.println(a+b); // addition 
      System.out.println(a-b); // substraction
      System.out.println(a*b); // multiplication
      System.out.println(a/b); // division 
      System.out.println(a%b);// modulo - remainder 
	}

}

package operators;

public class Assignment {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
	      System.out.println(a+=10); 
	      System.out.println(a*=10); 
	      System.out.println(a/=10); 
	      System.out.println(a-=10);

	}

}

package operators;

public class Logical {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(10>=10 && 10>=10);
		System.out.println(12>11 || 44>55);
	}

}

package operators;

public class Relational {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
	      int b = 15;
	      System.out.println(a>b); // greater than
	      System.out.println(a<b); // less than
	      System.out.println(a>=b); // greater than equal to
	      System.out.println(a<=b); // less than equal to
	      System.out.println(a==b); // equal to equal to
	      System.out.println(a!=b); // not equal

	}

}
